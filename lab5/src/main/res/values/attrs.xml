<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ChatMessageView">
        <attr name="message" format="string" />
        <attr name="host" format="string" />
    </declare-styleable>

    <declare-styleable name="ChooseQuestionView">
        <attr name="message" />
        <attr name="answer_id" format="integer" />
        <attr name="state" format="enum">
            <enum name="Default" value="0" />
            <enum name="Selected" value="1" />
            <enum name="Wrong" value="2" />
        </attr>
    </declare-styleable>
</resources>